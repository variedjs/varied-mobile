{
  "name": "@varied/mobile",
  "version": "0.1.1",
  "description": "一款轻量、可靠的移动端 Vue 组件库",
  "author": "Wuner",
  "private": false,
  "homepage": "https://github.com/variedjs/varied-mobile",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types",
    "vetur"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "npm run build:entry && webpack-dev-server --config build/webpack.site.dev.js",
    "build": "npx cross-env NODE_ENV=production webpack --config build/webpack.site.prd.js",
    "lint": "eslint ./docs --ext .js,.vue --fix && stylelint \"packages/*.css\"",
    "build:entry": "node build/build-entry.js",
    "build:lib": "node build/build-lib.js",
    "build:address": "node build/build-address-json.js",
    "build:getCom": "node build/get-components.js",
    "webpack:pro": "cross-env NODE_ENV=production webpack --color --config build/webpack.pkg.js\n",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clear-cache": "jest --clearCache",
    "test:coverage": "open test/coverage/index.html",
    "release": "sh build/release.sh",
    "release:site": "sh build/release-site.sh",
    "publish": "npm publish --access=public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "vant commit-lint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue}": [
      "eslint",
      "git add"
    ],
    "*.{vue,css,less}": [
      "stylelint",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "component"
  ],
  "dependencies": {
    "@babel/runtime": "7.x",
    "@vant/popperjs": "^1.0.2",
    "vue-lazyload": "1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.22",
    "@vant/compiler": "^1.0.0",
    "@vant/doc": "^2.6.1",
    "@vant/eslint-config": "^1.4.0",
    "@vant/markdown-loader": "^2.3.0",
    "@vant/touch-emulator": "^1.2.0",
    "@vue/babel-preset-jsx": "^1.1.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.7.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "dependency-tree": "^7.0.2",
    "eslint": "^6.6.0",
    "fast-glob": "^3.1.0",
    "file-loader": "^5.0.2",
    "gh-pages": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-jsonp": "^1.0.2",
    "postcss": "^7.0.21",
    "postcss-loader": "^3.0.0",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^3.0.0",
    "vue": "^2.6.12",
    "vue-jest": "4.0.0-beta.2",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
